<!-- All visualisations need a tooltip -->
<!-- It's avaiable for all partials if defined here -->
<script type="text/javascript">
var tooltip = d3.select("body")
  .append("div")
  .attr("class", "d3_tooltip")
  .style("position", "absolute")
  .style("z-index", "10")
  .style("visibility", "hidden")
  .text("placeholder");
</script>